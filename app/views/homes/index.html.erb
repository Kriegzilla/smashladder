<div class="row">
  <div class="large-12 columns">
    <div class="row">
      </div>
    </div>
    <div class="small-12 large-centered small-centered columns">
      <%= link_to image_tag("/SmashLadderSplash.png"), users_path %>
    </div>
    <div class="row">
      <% unless signed_in? %>
      <div class="large-6 small-12 columns">
        <div class="panel">
          <h5 class="text-center"><%= button_to "Sign Up", new_user_registration_path, method: :get, class: "button" %></h5>
        </div>
      </div>
      <div class="large-6 small-12 columns">
        <div class="panel">
          <h5 class="text-center"><%= button_to "Sign In", new_user_session_path, method: :get, class: "button" %></h5>
        </div>
      </div>
      <% end %>
    </div>
  </div>
</div>
